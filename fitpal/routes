from fitpal import app
from flask import render_template, url_for

@app.route('/')
def main():
    return render_template('home.html')

@app.route('/stats')
def user_stats():
    return render_template('stats.html')

@app.route('/goals')
def user_goals():
    return render_template('goals.html')

@app.route('/login')
def login():
    return render_template('login.html')
